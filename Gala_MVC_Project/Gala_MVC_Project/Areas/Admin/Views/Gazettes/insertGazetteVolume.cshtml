@model DAL.Models.GazetteVolumes

@{
    ViewBag.Title = "insertGazetteVolume";
    Layout = "~/Areas/Admin/Views/Shared/_Secomdary_Layout.cshtml";
}
<div class="container" style="background:white;">

    <div class="row" style="padding:50px 50px;">
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">
                <h4>GazetteVolumes</h4>
                <hr />
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="form-group">
                    @Html.LabelFor(model => model.GazetteVolume, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-10">
                        @Html.EditorFor(model => model.GazetteVolume, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.GazetteVolume, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.PublishedDate, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-10">
                        @Html.EditorFor(model => model.PublishedDate, new { htmlAttributes = new { @class = "form-control", id="datetimeid"} })
                        @Html.ValidationMessageFor(model => model.PublishedDate, "", new { @class = "text-danger" })
                    </div>
                </div>
                          

                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Create" class="btn btn-default" />
                    </div>
                </div>
            </div>
        }

        <div>
            @Html.ActionLink("Back to List", "Index")
        </div>
    </div>
</div>


@section scripts{
    
<link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
<script src="//code.jquery.com/jquery-1.10.2.js"></script>
<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {

            $('#datetimeid').datepicker();

        });
    </script>
    
    
    }